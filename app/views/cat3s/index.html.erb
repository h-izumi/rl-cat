<p id="notice"><%= notice %></p>

<h1>Cat3s</h1>

<table>
  <thead>
    <tr>
      <th>Cat2</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cat3s.each do |cat3| %>
      <tr>
        <td><%= cat3.cat2 %></td>
        <td><%= cat3.name %></td>
        <td><%= link_to 'Show', cat3 %></td>
        <td><%= link_to 'Edit', edit_cat3_path(cat3) %></td>
        <td><%= link_to 'Destroy', cat3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cat3', new_cat3_path %>
