<p id="notice"><%= notice %></p>

<h1>Cat2s</h1>

<table>
  <thead>
    <tr>
      <th>Cat1</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cat2s.each do |cat2| %>
      <tr>
        <td><%= cat2.cat1 %></td>
        <td><%= cat2.name %></td>
        <td><%= link_to 'Show', cat2 %></td>
        <td><%= link_to 'Edit', edit_cat2_path(cat2) %></td>
        <td><%= link_to 'Destroy', cat2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cat2', new_cat2_path %>
